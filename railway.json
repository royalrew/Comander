{
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "pip install -r requirements.txt",
        "watchPatterns": [
            "commander-core/**"
        ]
    },
    "deploy": {
        "startCommand": "cd commander-core && python main.py",
        "numReplicas": 1,
        "restartPolicy": "ALWAYS"
    }
}