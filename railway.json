{
    "$schema": "https://railway.app/v2/railway.schema.json",
    "build": {
        "builder": "NIXPACKS",
        "watchPatterns": [
            "commander-core/**"
        ]
    },
    "deploy": {
        "startCommand": "cd commander-core && python main.py",
        "numReplicas": 1,
        "restartPolicy": "ALWAYS"
    }
}